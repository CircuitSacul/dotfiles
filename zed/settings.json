{
  "features": {
    "inline_completion_provider": "none"
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "Catppuccin Mocha"
  },
  "vim_mode": true,
  "terminal": {
    "font_family": "MesloLGS NF"
  },
  "ui_font_family": "MesloLGS NF",
  "buffer_font_family": "MesloLGS NF",
  "lsp": {
    "rust-analyzer": {
      "initialization_options": {
        "checkOnSave": {
          "command": "clippy"
        }
      }
    }
  }
}
